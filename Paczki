public class Paczki extends Thread 
{
	private DostawcaPaczków dostawcaPaczków;
	
	public Paczki(DostawcaPaczków dostawcaPaczków)
	{
		this.dostawcaPaczków = dostawcaPaczków;
	}
	
//	private void komunikat(String text)
//	{
//		System.out.println(" ");
//	}
	
	public void run()
	{
		while(true)
		{
			dostawcaPaczków.paczki_ok_1();
			dostawcaPaczków.paczki_ok_2();
			try
			{
				sleep(ProgramGlowny.CZAS);
			}
			catch (InterruptedException e) 
			{
				e.printStackTrace();
			}
		}
	}
}
